#include <stdio.h>
using namespace std;

int price[4];
int month[13];
int dMonth[13];
int d[13];

int min(int a, int b) {
	return (a < b) ? a : b;
}

int main() {

	int tc;
	scanf("%d", &tc);

	for (int t = 1; t <= tc; t++) {
		for (int i = 0; i < 4; i++) {
			scanf("%d", &price[i]);
		}

		for (int i = 1; i <= 12; i++) {
			scanf("%d", &month[i]);
		}

		//min(하루x날, 한달)
		for (int i = 1; i <= 12; i++) {
			dMonth[i] = min(price[0] * month[i], price[1]);
		}

		for (int i = 1; i <= 12; i++) {
			d[i] = d[i - 1] + dMonth[i];
			if (i - 3 >= 0) {
				if (d[i] > d[i - 3] + price[2]) {
					d[i] = d[i - 3] + price[2];
				}
			}
		}

		if (d[12] > price[3]) {
			d[12] = price[3];
		}

		printf("#%d %d\n", t, d[12]);
	}
}
